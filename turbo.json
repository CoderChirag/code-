{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**", ".next", "!.next/cache", "storybook-static/**"],
      "dependsOn": ["^build"]
    },
    "start": {
      "dependsOn": ["^start"],
      "cache": false
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  }
}
